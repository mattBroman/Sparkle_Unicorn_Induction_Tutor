{
  "name": "parser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pegjs -o parser.js --format globals -e PARSER grammar.pegjs",
    "test": "pegjs --allowed-start-rules '_, _Newline, _Numeral, _Term, _Expression, _Expression_AS, _Expression_MD, _Expression_E, _Expression_P, _EquivalenceExpression, _Assumption, _InductiveStep, _BaseCase, _AssumeBlock, _ShowBlock, _UseIHBlock, _Proof, _Root' -o parser.js grammar.pegjs && ava"
  },
  "ava": {
    "files": [
      "tests/**/*.js",
      "tests/**/_*.js",
      "!tests/common.js"
    ],
    "failFast": true
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.25.0",
    "pegjs": "^0.10.0"
  }
}
