<h2>Users</h2>

<table>
  <tr>
    <th>Name</th>
    <th>Role</th>
   </tr>
   <% @users.each do |u| %>
   <tr>
    <td> <%= u.name %> </td>
    <td> <%= u.role %> </td>
    <td> <%= link_to 'user', user_path(u) %></td>
    <td> <%#= link_to 'delete', delete_user_path(u)%></td>
  </tr>
  <% end %>
</table>

<%= link_to 'new user', new_user_path %>