<% content_for :include, javascript_include_tag('none-bundle', 'data-turbolinks-track': 'reload'), :flush => true%>

<h2>All the questions</h2>
<h3>makes sure y'all add some becuase rn its blank</h3>
<table>
  <tr>
    <th>Title</th>
    <th>Difficulty</th>
   </tr>
   <% @questions.each do |question| %>
   <tr>
    <td> <%= question.title %> </td>
    <td> <%= question.difficulty %> </td>
    <td> <%= link_to 'proof', question_path(question) %></td>
    <td> <%= link_to 'delete', delete_question_path(question)%></td>
  </tr>
  <% end %>
</table>
<p> <%= link_to 'new question', new_question_path %> </p>
<p> <%= link_to 'home', welcome_index_path %></p>
<% if flash[:notice] %>
  <p><%= flash[:notice] %></p>
<% end %>


    